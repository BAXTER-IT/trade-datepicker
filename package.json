{
  "name": "trade-datepicker",
  "version": "3.0.22",
  "description": "Angular datepicker and date range picker",
  "keywords": [
    "Angular",
    "datepicker",
    "date range picker"
  ],
  "author": "baxter",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BAXTER-IT/trade-datepicker.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-lib": "node scripts/delete-dist-folder.js && ng build AngularMyDatePicker && node scripts/copy-artifacts.js",
    "build-all": "node scripts/delete-dist-folder.js && ng build AngularMyDatePicker && node scripts/copy-artifacts.js && ng build",
    "test-lib": "node scripts/delete-test-output-folder.js && ng test --code-coverage=true AngularMyDatePicker",
    "lint-lib": "ng lint AngularMyDatePicker",
    "e2e": "ng e2e"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~15.2.8",
    "@angular/common": "~15.2.8",
    "@angular/compiler": "~15.2.8",
    "@angular/core": "~15.2.8",
    "@angular/forms": "~15.2.8",
    "@angular/platform-browser": "~15.2.8",
    "@angular/platform-browser-dynamic": "~15.2.8",
    "@angular/router": "~15.2.8",
    "core-js": "^3.30.1",
    "rollup": "^3.20.6",
    "rxjs": "~7.8.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.6",
    "@angular/cli": "~15.2.6",
    "@angular/compiler-cli": "~15.2.8",
    "@angular/language-service": "~15.2.8",
    "@types/jasmine": "~4.3.1",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "~8.9.4",
    "codecov": "^3.5.0",
    "codelyzer": "^6.0.2",
    "fs-extra": "^8.0.1",
    "jasmine-core": "~4.6.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ng-packagr": "^15.2.2",
    "protractor": "^7.0.0",
    "ts-node": "~7.0.0",
    "tsickle": "0.46.3",
    "tslib": "^2.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.9.5"
  }
