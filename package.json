{
  "name": "trade-datepicker",
  "version": "3.0.19",
  "description": "Angular datepicker and date range picker",
  "keywords": [
    "Angular",
    "datepicker",
    "date range picker"
  ],
  "author": "baxter",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BAXTER-IT/trade-datepicker.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-lib": "node scripts/delete-dist-folder.js && ng build AngularMyDatePicker && node scripts/copy-artifacts.js",
    "build-all": "node scripts/delete-dist-folder.js && ng build AngularMyDatePicker && node scripts/copy-artifacts.js && ng build",
    "test-lib": "node scripts/delete-test-output-folder.js && ng test --code-coverage=true AngularMyDatePicker",
    "lint-lib": "ng lint AngularMyDatePicker",
    "e2e": "ng e2e"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "core-js": "^2.5.4",
    "rollup": "^2.35.1",
    "rxjs": "~6.5.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/language-service": "~13.3.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codecov": "^3.5.0",
    "codelyzer": "^6.0.2",
    "fs-extra": "^8.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^13.1.3",
    "protractor": "^7.0.0",
    "ts-node": "~7.0.0",
    "tsickle": "0.46.0",
    "tslib": "^2.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.5.2"
  }
